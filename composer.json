{
  "name": "davidhirtz/yii2-media-s3",
  "type": "yii2-extension",
  "description": "Yii2 Media Amazon S3 extension",
  "homepage": "https://www.davidhirtz.com/",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "davidhirtz/yii2-media": "^v1.3.1",
    "aws/aws-sdk-php": "^3.154"
  },
  "autoload": {
    "psr-4": {
      "davidhirtz\\yii2\\media\\s3\\": ""
    }
  },
  "extra": {
    "bootstrap": "davidhirtz\\yii2\\media\\s3\\composer\\Bootstrap"
  }
}